# game/script.rpy:5300
translate vietnamese day1_watered_5_62266673:

    # m "There's something I need to say."
    m "Có điều gì đó tớ cần nói."

# game/script.rpy:5302
translate vietnamese day1_watered_5_80aa413a:

    # b "Ok."
    b "Ok."

# game/script.rpy:5304
translate vietnamese day1_watered_5_fe8a7d5b:

    # m "What if... I eat you tomorrow? And we can go out again?"
